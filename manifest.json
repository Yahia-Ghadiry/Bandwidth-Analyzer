{

  "manifest_version": 2,
  "name": "Bandwidth Analizer TEST",
  "version": "1.3.1",
  "homepage_url":"https://noneYet.com",
  "description": "A tool that helps you analyze used data to help you not exceed your monthly data cap",

  "icons":
  {
      "48": "/icons/tmp.png"
  },


  "browser_action":
  {
      "default_icon": "/icons/tmp.png",
      "default_title": "Bandwidth Usage Analizer",
      "default_popup": "/popup/popup.html"
  },


  "background":
  {
      "scripts": ["/src/dexie.js", "/background/background.js"]
  },


  "permissions": [
      "tabs",
      "storage",
      "unlimitedStorage",
      "webRequest",
      "<all_urls>"
  ]

}